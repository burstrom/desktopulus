# GNU Make solution makefile autogenerated by Premake

ifndef config
  config=linux32_debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),linux32_debug)
  testomatic_config = linux32_debug
endif
ifeq ($(config),linux64_debug)
  testomatic_config = linux64_debug
endif
ifeq ($(config),linux32_release)
  testomatic_config = linux32_release
endif
ifeq ($(config),linux64_release)
  testomatic_config = linux64_release
endif

PROJECTS := testomatic

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

testomatic:
ifneq (,$(testomatic_config))
	@echo "==== Building testomatic ($(testomatic_config)) ===="
	@${MAKE} --no-print-directory -C . -f testomatic.make config=$(testomatic_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f testomatic.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  linux32_debug"
	@echo "  linux64_debug"
	@echo "  linux32_release"
	@echo "  linux64_release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   testomatic"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
